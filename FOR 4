import java.util.Scanner;

public class for4 {

	public static void main(String[] args) {
		Scanner scan = new Scanner (System.in);
		int a = scan.nextInt();
		int b = scan.nextInt();
		int oddcount = 0;
		int i;
		for(i=a;i<=b;i++)
		{
			if(i%2==1)
			{
				oddcount++;
			}
		}     
			System.out.println(oddcount);

	}

}
