import java.util.Scanner;

public class while4 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
				int num = scan.nextInt();
		        int sum = 0;
		        while(num>0)
		        {
		        	int temp = num%10;
		        	sum=sum+temp;
		        	num=num/10;
		        }
		        System.out.println(sum);
	}

}
